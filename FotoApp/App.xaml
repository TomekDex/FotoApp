<Application x:Class="FotoApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FotoApp">
    <Application.Resources>
        <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="Resources/TextStyle.xaml"/>
                    <ResourceDictionary Source="Resources/ForoAppCheckBox.xaml"/>
                    <ResourceDictionary Source="Resources/FotoAppButton.xaml"/>
                    <ResourceDictionary Source="Resources/FotoAppRadioButton.xaml"/>
                    <ResourceDictionary Source="Resources/BrushColor.xaml"/>
                    <ResourceDictionary Source="Resources/FotoDataTemplate.xaml"/>
                <ResourceDictionary>
                    <local:AppBootstrapper x:Key="Bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>            
        </ResourceDictionary>
    </Application.Resources>
</Application>
